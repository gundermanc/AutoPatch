             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AutoPatcher">
    <Application.MainWindow>
        <local:MainWindow Visibility="Visible">
            <local:MainWindow.DataContext>
                <local:AppModel />
            </local:MainWindow.DataContext>
        </local:MainWindow>
    </Application.MainWindow>
</Application>
